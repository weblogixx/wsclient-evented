    "websocket client",
    "event"
  ],
  "author": "Christian Schilling (cs@weblogixx.de)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/weblogixx/wsclient-evented/issues"
  },
  "homepage": "https://github.com/weblogixx/wsclient-evented#readme",
  "devDependencies": {
    "babel-core": "^5.8.22",
    "babel-loader": "^5.3.2",
    "chai": "^3.2.0",
    "eslint": "^1.2.1",
    "eslint-loader": "^1.0.0",
    "http-server": "^0.8.5",
    "isparta-loader": "^1.0.0",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.1",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.0",
    "karma-mocha-reporter": "^1.1.1",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-phantomjs-shim": "^1.1.1",
    "karma-safari-launcher": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-webpack": "^1.7.0",
    "karma-websocket-server": "^1.0.0",
    "minimist": "^1.2.0",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.18",
    "webpack": "^1.12.0"
  },
  "dependencies": {
    "fbemitter": "^2.0.0"
  }
